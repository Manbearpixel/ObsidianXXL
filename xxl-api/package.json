  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Manbearpixel/xxl-api/issues"
  },
  "homepage": "https://github.com/Manbearpixel/xxl-api#readme",
  "dependencies": {
    "async": "^2.5.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "minimist": "^1.2.0",
    "moment": "^2.19.2",
    "morgan": "^1.9.0",
    "node-json-rpc2": "^1.1.0",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "request": "^2.83.0",
